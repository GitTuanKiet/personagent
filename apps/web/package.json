{
  "name": "web",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --port 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --max-warnings 0",
    "check-types": "tsc --noEmit",
    "db:generate": "dotenv -e .env.local drizzle-kit generate",
    "db:migrate": "dotenv -e .env.local drizzle-kit migrate",
    "db:studio": "dotenv -e .env.local drizzle-kit studio",
    "db:push": "dotenv -e .env.local drizzle-kit push",
    "db:drop": "dotenv -e .env.local drizzle-kit drop",
    "client:db:migrate": "IN_CLIENT=true dotenv -e .env.local drizzle-kit generate && bun scripts/migrations.ts",
    "client:db:drop": "IN_CLIENT=true dotenv -e .env.local drizzle-kit drop"
  },
  "dependencies": {
    "@electric-sql/pglite": "^0.3.3",
    "@fingerprintjs/fingerprintjs": "^4.6.2",
    "@hookform/resolvers": "^5.1.1",
    "@langchain/core": "^0.3.57",
    "@pag/langgraph-bua": "workspace:*",
    "@tanstack/react-query": "^5.80.6",
    "@trpc/client": "^11.3.1",
    "@trpc/next": "^11.3.1",
    "@trpc/react-query": "^11.3.1",
    "@trpc/server": "^11.3.1",
    "@workspace/ui": "workspace:*",
    "drizzle-orm": "^0.44.2",
    "electron": "^36.4.0",
    "lucide-react": "^0.514.0",
    "next": "latest",
    "next-themes": "^0.4.6",
    "patchright": "^1.52.5",
    "pino": "^9.7.0",
    "playwright": "^1.53.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.57.0",
    "recharts": "^2.15.3",
    "sonner": "^2.0.5",
    "superjson": "^2.2.2",
    "swr": "^2.3.3",
    "zod": "^3.25.64",
    "zustand": "^5.0.5",
    "zustand-utils": "^2.1.0"
  },
  "devDependencies": {
    "@pag/eslint-config": "workspace:*",
    "@pag/typescript-config": "workspace:*",
    "@types/node": "*",
    "@types/react": "19.1.7",
    "@types/react-dom": "19.1.6",
    "dotenv-cli": "^8.0.0",
    "drizzle-kit": "^0.31.1",
    "postgres": "^3.4.7",
    "typescript": "^5.8.3"
  }
}